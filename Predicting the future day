import java.util.Scanner;

public class WorkSpace{
public static void main(String[] args)
   {
       Scanner kb = new Scanner(System.in);
       System.out.print("Enter day (0-6) ");
              int d = kb.nextInt();
       System.out.print("Enter days later ");
              int dl = kb.nextInt();
              
              int fd = (d+dl) % 7;
              if(fd == 0)
                  System.out.println(dl+" Days later is S");
              else if (fd == 1)
                  System.out.println(dl+" Days later is M");
              else if (fd == 2)
                  System.out.println(dl+" Days later is T");
              else if (fd == 3)
                  System.out.println(dl+" Days later is W");
              else if (fd == 4)
                  System.out.println(dl+" Days later is TH");
              else if (fd == 5)
                  System.out.println(dl+" Days later is F");
              else
                  System.out.println(dl+" Days later is Sa");

      }
} 
